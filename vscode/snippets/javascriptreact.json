{
	// Place your snippets for javascriptreact here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }

	"Print to console": {
		"prefix": [
			"cl",
			"log"
		],
		"body": [
			"console.log($1)"
		],
		"description": "Log output to console"
	},

	"Import React": {
		"prefix": [
			"ir",
			"impreact",
			"importreact",
		],
		"body": [
			"import React from 'react'",
		],
		"description": "import react"
	},

	"Import ReactDOM": {
		"prefix": [
			"ird",
			"impreactdom",
			"importreactdom"
		],
		"body": [
			"import ReactDOM from 'react-dom'",
		],
		"description": "import react-dom"
	},

	"Import React-Router": {
		"prefix": [
			"irr",
			"impreactrouter",
			"importreactrouter"
		],
		"body": [
			"import { HashRouter, Route, Switch } from 'react-router-dom'",
		],
		"description": "import react-router-dom"
	},

	"Import styled-components": {
		"prefix": [
			"istyled",
			"impstyled",
			"importstyled"
		],
		"body": [
			"import styled from 'styled-components'",
		],
		"description": "import styled-components"
	},

	"Import styled-components-css": {
		"prefix": [
			"istyledcss",
			"impstyledcss",
			"importstyledcss"
		],
		"body": [
			"import { css } from 'styled-components'",
		],
		"description": "import { css } from styled-components"
	},

	"React class component template": {
		"prefix": [
			"rclass",
			"reactclass"
		],
		"body": [
			"import React, { Component } from 'react'",
			"",
			"class $1 extends Component {",
			"\trender() {",
			"\t\treturn (",
			"\t\t)",
			"\t}",
			"}",
			"",
			"export default $1"
		],
		"description": "react class component template"
	},

	"react function component ()": {
		"prefix": [
			"rf()",
			"reactfunction()",
		],
		"body": [
			"const $1 = props => (",
			")",
			"",
			"export default $1"
		],
		"description": "const ... = props => ( ... )"
	},

	"react function component export default()": {
		"prefix": [
			"edrf()",
			"exportdefaultreactfunction()",
		],
		"body": [
			"export default props => (",
			")",
			"",
		],
		"description": "export default props => ( ... )"
	},

	"react function component {}": {
		"prefix": [
			"rf{}",
			"reactfunction{}",
		],
		"body": [
			"const $1 = props => {",
			"\treturn (",
			"\t)",
			"}",
			"",
			"export default $1"
		],
		"description": "const ... = props => { ... }"
	},

	"react function component export default {}": {
		"prefix": [
			"edrf{}",
			"exportdefaultreactfunction{}",
		],
		"body": [
			"export default props => {",
			"\treturn (",
			"\t)",
			"}",
			"",
		],
		"description": "export default props => { ... }"
	},

	"export default": {
		"prefix": [
			"ed",
			"exd",
			"expd",
			"expdef"
		],
		"body": [
			"export default ",
		],
		"description": "export default ..."
	},

	"styled component": {
		"prefix": [
			"styled",
		],
		"body": [
			"const $1 = styled.div`",
			"`"
		],
		"description": "const ... = styled.div`...`"
	},
}